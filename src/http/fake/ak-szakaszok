            location.href=$(this).closest('div').data('url');
        });

        $('.accordion-container .accordion-title').on('click',function(){
            if(!$(this).closest('.accordion-item').hasClass('opened')){
                $(this).closest('.accordion-container').find('.accordion-item').removeClass('opened');
                $(this).closest('.accordion-container').find('.accordion-item .accordion-content').not(':hidden').slideUp();
    
                $(this).closest('.accordion-item').addClass('opened');
                $(this).closest('.accordion-item').find('.accordion-content').slideDown();
            }
        });
        $('.footer-content .logo').on('click',function(){
            if($('.mtszmenu-row').hasClass('hidden')){
                $('.mtszmenu-row').removeClass('hidden');
            }else {
                $('.mtszmenu-row').addClass('hidden');
            }
        });
        
        
    

