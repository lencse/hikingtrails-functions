                dots: false,
                pauseOnHover: true,
                cssEase: 'linear',
               // asNavFor: '#'+$coverid, //'.cover > .slider-wrapper',
                focusOnSelect: true,
                responsive: [
                    {
                        breakpoint: 1200,
