                if(!$(this).attr('href').match(/http/g)) {
                    //console.log('menu is not a link');
                  e.preventDefault();
                  if($(this).closest('li').hasClass('open')) $(this).closest('li').removeClass('open').removeClass('active');
                  else $(this).closest('li').addClass('open');
                
                  //if($(this).closest('li').find('ul').hasClass('opened')) $(this).closest('li').find('ul').removeClass('opened');
