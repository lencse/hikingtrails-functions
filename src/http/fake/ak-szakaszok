<section class="tour-detail-page show-xs">
    <div class="container">

        <div class="row buttons">



                        <div class="col-md-6 col-sm-6 col-xs-6 padding-right-5"><a class="btn full-width" href="https://turistaterkepek.hu/kekturahu/gpx/mozgalom/ak_teljes_20211014.gpx" target="_blank"><span class="row1">Nyomvonal</span><span class="row2">GPX - 2021.10.14.</span></a></div>
            <div class="col-md-6 col-sm-6 col-xs-6 padding-left-5"><a class="btn full-width" href="https://turistaterkepek.hu/kekturahu/gpx/mozgalom/ak_bh_20211111.gpx" target="_blank"><span class="row1">Bélyegzőhelyek</span><span class="row2">GPX - 2021.11.11.</span></a></div>

            <div class="col-md-12 col-xs-12"><a class="btn full-width" href="https://turistaterkepek.hu/kekturahu/gpx/mozgalom/ak_teljes_bh_20211111.gpx" target="_blank"><span class="row1">Nyomvonal bélyegzőhelyekkel</span><span class="row2">GPX - 2021.11.11.</span></a></div>
            <div class="col-md-6 col-sm-6 col-xs-6 padding-right-5"><a class="btn full-width" href="https://turistaterkepek.hu/kekturahu/gpx/tablazatok/AK_aktualis_belyegzohelyek_20211111.pdf" target="_blank"><span class="row1">Bélyegzőhelyek</span><span class="row2">PDF - 2021.11.11.</span></a></div>
                        <div class="col-md-6 col-sm-6 col-xs-6 padding-left-5"><a class="btn full-width" href="https://turistaterkepek.hu/kekturahu/gpx/tablazatok/AK_current_stamping_locations_20211111.pdf" target="_blank"><span class="row1">Stamping locations</span><span class="row2">PDF - Nov 11, 2021</span></a></div>
                        <div class="col-md-12  col-xs-12"><p><a class="btn full-width" href="https://turistaterkepek.hu/kekturahu/tablazatok/ak_szakasz_adatok.pdf" target="_blank" rel="noopener noreferrer"><span class="row1">Szakaszadatok</span><span class="row2">PDF - 2021.10.14.</span></a>
	<a class="btn full-width" href="http://mtsz.org/images/arg_attachment/2/2833/ak%20kieg%C3%A9sz%C3%ADt%C5%91lap%203a%20%202020%200921.pdf?99756700617907941" target="_blank" rel="noopener noreferrer"><span class="row1">bélyegzőhelyek pótlap</span><span class="row2">PDF - 2021.01.14.</span></a>
</p></div>

            <div class="col-md-12  col-xs-12"><div class="description"><p style="text-align: justify;">Nemzeti parkok és tájvédelmi körzetek egyedülálló természeti  értékeit ismerheted meg akkor, ha az Országos Kékkör harmadik és egyben  legfiatalabb útvonalán túrázol. Az MTSZ és az  alföldi tagszervezetek közös munkájának gyümölcse 1996-ban került átadásra a  nagyközönségnek. <em><a href="https://www.kektura.hu/hir/uton-lenni-fontosabb-mint-odaerni" target="_blank">Gyenes Károly:</a> Kerekek és lépések</em> című  sorozata, egyben a kéktúra filmek utolsó része a végtelen rónaság kék útját  mutatja be a nézőknek. Az Alföldi Kéktúra egyes szakaszai együtt haladnak a Magyarországon áthaladó E3, E4 és E7 jelzésű, hosszútávú nemzetközi turistautakkal.</p><p style="text-align: justify;">A kéktúrák speciális egysége a <a href="https://www.kektura.hu/tudastar-cikk/mit-jelent-azhogy-szakasz" target="_blank">szakasz</a>. Ezen a honlapon szakaszok szerint is tudsz keresni és tervezni.<br></p></div></div>


        </div>
    </div>
</section>
<script type="text/javascript">
    var circles = [];
    var panelTimeout;
    var szakaszidk = ['587156','587169','587168','587167','587166','587165','587164','587175','587174','587172','587173','587171','587170'];
    $(function(){
        $(document).off('mouseover click', '.hover-row');
        $(document).on('mouseover', '.hover-row', function(e){
            e.preventDefault();
            var $active = $(this);
            if (!L.Browser.mobile) {
                $('.hover-row').removeClass('active');
                $active.addClass('active');
                highlightOnMap($active.data('element'))
            }
        });
        $(document).on('click', '.hover-row', function(e){
            e.preventDefault();
            var $active = $(this);
            if (L.Browser.mobile) {
                if ($active.hasClass('mobile-active')) {
                    if($(this).data('url')) location.href=$(this).data('url');
                } else {
                    $('.hover-row').removeClass('active mobile-active');
                    $active.addClass('active mobile-active');
                    highlightOnMap($active.data('element'));
                }
            } else {
                if($(this).data('url')) location.href=$(this).data('url');
            }

        });
    });

    function highlightOnMap(id, removeonly) {
        var circle = false;
        $.each(circles, function(i,e){
            e.setStyle({fillOpacity:0});
            if (e.options.trackelement == id) {
                circle = e;
                if (removeonly) {
                    window['polyline'+id].setStyle({
                        color: window['polyline'+e.options.trackelement].options.originalColor,
                        opacity: 0.8,
                        weight: 4
                    });
                } else {
                    window['polyline'+id].setStyle({
                        color: 'black',
                        opacity: 0.7,
                        weight: 5
                    });
                }
            } else {
                window['polyline'+e.options.trackelement].setStyle({
                    color: window['polyline'+e.options.trackelement].options.originalColor,
                    opacity: 0.8,
                    weight: 4
                });
            }
        });

        highlightPanel(id, removeonly);
        if (removeonly!==true && circle!==false) circle.setStyle({fillOpacity:0.4});
    }

    function showPanel(id, removeonly) {
        var $panel = $('.szakasz-info-panel[data-element="'+id+'"]');
        $('.szakasz-info-panel').removeClass('active');
        if (removeonly!==true) {
            $panel.addClass('active');
        } else {
            $('.szakasz-info-panel[data-element="0"]').addClass('active');
        }
    }

    function highlightPanel(id, removeonly) {
        if (panelTimeout) {
            clearTimeout(panelTimeout);
            panelTimeout = false;
        }
        panelTimeout = setTimeout(function(){ showPanel(id, removeonly) }, 250);
    }

    function highlightRow(id, removeonly) {
        var $row = $('.hover-row[data-element="'+id+'"]');
        $('.hover-row').removeClass('active');
        highlightPanel(id, removeonly);
        if (removeonly!==true) {
            scrollToRow($row);
            $row.addClass('active');
        }
    }
    var hunBounds = L.latLngBounds(L.latLng(45.7594811061, 16.2022982113), L.latLng(48.546320, 22.943268));
    var tourBounds = L.latLngBounds();
    var map = L.map('map', {
        zoomSnap: 0.1,
        zoomControl: false,
        //maxBounds: hunBounds.pad(0.05),
        gestureHandling: true,
        gestureHandlingOptions: {
            text: {
                touch: false,
                scroll: false,
                scrollMac: false
            }
        },
        'dragging': false,
        'tap': false
    });
    map.setMinZoom(map.getBoundsZoom(hunBounds.pad(0.05)));
    //map.fitBounds(hunBounds);
    L.esri.dynamicMapLayer({
        url: 'https://turistaterkepek.hu/serverwa/rest/services/orszagos_kektura/magyarorszag/MapServer',
        //url: 'https://hikingmaps.hu:6443/arcgis/rest/services/orszagos_kektura/magyarorszag/MapServer',
        opacity: 1,
    }).addTo(map);
    map.touchZoom.disable();
    map.doubleClickZoom.disable();
    map.scrollWheelZoom.disable();
    map.boxZoom.disable();
    map.keyboard.disable();
    /*$(window).resize(function(){
        map.invalidateSize();
        map.fitBounds(hunBounds);
    });*/

    function drawSzakasz(d) {
        var center = new L.latLng(d.center);
        var radi = 0;
        $.each(d.points, function(i,e){
            dist = center.distanceTo(e)+2000;
            if (dist>radi) radi = dist;
        });
        if (radi<10000) radi = 10000;

        circles.push(new L.Circle(d.center, {
            radius: radi,
            color: d.color,
            stroke: false,
            opacity: 0,
            fillOpacity: 0,
            zIndexOffset: 5,
            trackelement: d.id,
            interactive: false
        }).addTo(map));
        window['outline'+d.id] = new L.Polyline(d.points, {
            color: 'white',
            weight: 5,
            lineCap: 'square',
            zIndexOffset: 600,
            opacity: 0.5
        }).addTo(map);
        tourBounds.extend(window['outline'+d.id].getBounds());
        window['polyline'+d.id] = new L.Polyline(d.points, {
            color: d.color,
            originalColor: d.color,
            weight: 4,
            lineCap: 'square',
            zIndexOffset: 601,
            opacity: 0.8
        }).addTo(map);
        window['overline'+d.id] = new L.Polyline(d.points, {
            color: 'white',
            weight: 20,
            opacity: 0,
            lineCap: 'square',
            link: d.link,
            zIndexOffset: 602,
            elementid: d.id
        }).addTo(map).on('mouseover', function(e) {
            highlightOnMap(e.target.options.elementid);
            highlightRow(e.target.options.elementid);
        }).on('mouseout', function(e) {
            highlightOnMap(e.target.options.elementid, true);
            highlightRow(e.target.options.elementid, true);
        }).on('click', function(e){
            //window.location.href = e.target.options.link;
            $('.hover-row[data-element="'+e.target.options.elementid+'"]').trigger('click');
        });
    }
    $(function(){
        $.ajax({
            url: '/action/do/595',
            type: 'get',
            cache: true,
            dataType: 'json',
            data: {szakasz: szakaszidk, mozg: 'AK' ,QVALIDATION: $('input[name="QVALIDATION"]').val(),usecolor:0},
            success: function(d) {
                if (d.result) {
                    $.each(d.data, function(i,data){
                        drawSzakasz(data);
                    });
                    map.fitBounds(tourBounds.pad(0.02));
                    map.setMaxBounds(tourBounds.pad(0.02));
                    map.setMaxZoom(map.getBoundsZoom(tourBounds.pad(0.02)));
                    map.setMinZoom(map.getBoundsZoom(tourBounds.pad(0.02)));
                }
            }
        });
    });

</script></div>
<script type="text/javascript">
	</script>
