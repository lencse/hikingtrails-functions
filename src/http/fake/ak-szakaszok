             }).on('beforeChange', function (event, slick, currentSlide, nextSlide) {
                 
               //$coverNavigationSlider.find('.datastore-insert').remove();
              
                 
             }).trigger("beforeChange", [null, 0, 0]).on('afterChange',function(event, slick, currentSlide, nextSlide){
