                preload: 2,
                buttons: true
            });
        });

        $('.form-content #inp_equal_ship').on('click', function(event) {
            //event.preventDefault();
            //event.stopPropagation();

            if($(this).is(':checked')){
                $('#bill_data_summary').removeClass('hidden');
                $('#bill_data_content').addClass('hidden');
            }
            else {
                $('#bill_data_summary').addClass('hidden');
                $('#bill_data_content').removeClass('hidden');
            }
        });

        checkTicketState();
    });

    function checkTicketState() {
        $('.ticket.state .percent').each(function(event) {
            var percent = $(this).data('percent');
            $(this).css({'width':percent+'%'});
            var $this = $(this);
            setTimeout(function(){$this.html(percent+'%');},500);
        });


    }

    function setHeightByRatio() {
        var ratio;
        //console.log('setHeightByRatio');
        if ($('.setratio').length)
        {
            $('body .setratio').each(function () {
                if(($(window).width()>=768 && $(this).data('ratio')) || ($(window).width()<768 && ($(this).data('mobileratio') || $(this).data('ratio')))) {
                    var hcorrection = $(this).data('heightcorrection') ? $(this).data('heightcorrection') : 0;
                    var wcorrection = $(this).data('widthcorrection') ? $(this).data('widthcorrection') : 0;
                    ratio = $(this).data('ratio')?$(this).data('ratio'):1;
                    if($(window).width()>=768 && $(this).data('ratio')) ratio = $(this).data('ratio');
                    else if($(window).width()<768 && $(this).data('mobileratio')) ratio = $(this).data('mobileratio');

                    var h = (($(this).find('.image').width()+wcorrection)*ratio)+hcorrection;
                    if($(this).hasClass('event-list-item')) h = (($(this).find('> a').width()+wcorrection)*ratio)+hcorrection;
                    else if($(this).find('.image').length==0) {
                        h = (($(this).width()+wcorrection)*ratio)+hcorrection;
                    }
                    //console.log($(this).attr('id')+' -> '+ratio +' hc: '+hcorrection+' wc: '+wcorrection, 'wh:'+$(this).find('.image').width()+'x'+h);
                    $(this).find('.image').css({'height': h+'px'});

                    if($(this).hasClass('event-list-item')) {
                        //$(this).find('> a').css({'height': h});
                        $(this).css({'height': h});
                        $(this).closest('section.event-list').css({'min-height': h});
                    }
                    else if($(this).hasClass('event-list')) {
                        $(this).closest('section.event-list').css({'min-height': h});
                    }
                    else {
                        //$(this).css({'height': h});
                    }

                }
            });

        }

        if ($('.setimgbg').length)
        {
            $('body .setimgbg').each(function () {
                $(this).find('.image').css({'background-image': 'url('+($(this).find('img').attr('data-src')?$(this).find('img').attr('data-src'):$(this).find('img').attr('src'))+')'});
            });
        }

        if($('.image-block').length) {
            $('body .image-block').each(function () {
                img = $(this).find('.wrapper img');
                //allo
                if($(img[0]).width()<$(img[0]).height()) {
                    $(this).addClass('portrait');
                }
                if($(this).hasClass('portrait')) {
                    $(this).find('.wrapper').css({'height':$(this).width()});
                    $(img[0]).css({'height':$(this).width()});
                }
            });

        }
        
    }

    function scrollTo(hash) {
        var target = hash,
            $target = $(target),
            topOffset = $('header').outerHeight();

        if ($target.offset()) {
            $('html, body').stop().animate({
                'scrollTop': Math.max(0, $target.offset().top - topOffset)
            }, 600, 'swing');
        }
    }
    
    function checkSiteHeight(){
        if($('.main-container').length){
            $('.main-container').css({'min-height':1});
            h = $('.header-container').outerHeight()+$('.main-container').outerHeight()+$('footer').outerHeight();
            if($(window).height()>h) {
                //console.log($(window).height()+'>'+h);
                $('.main-container').css({'min-height':parseInt($('.main-container').outerHeight()+$(window).height()-h)})
            }
        } 
    }


        $('.map-tour-selector .map-container .button').hover(function(){
            if(!$(this).hasClass('active')){
                $('.map-tour-selector .map-container .button').removeClass('active');
                $(this).addClass('active');
                target = $(this).data('target');
                $('.map-tour-selector .tour-item').removeClass('active');
                $('.map-tour-selector #'+target).addClass('active');
            }
        });

        $('.map-tour-selector .map-container .button').on('click',function(){
            var target = $(this).data('target');
            var href = $(this).data('href');
            //$('.map-tour-selector #'+target+' .icon').clone().appendTo( '.map-tour-selector #'+target).addClass('flyable').css({'position':'fixed','z-index':1000,'top':$('.map-tour-selector #'+target+' .icon').offset().top,'left':$('.map-tour-selector #'+target+' .icon').offset().left});
            if($(window).width()>768) {
                $('.map-tour-selector #'+target+' .icon').clone().appendTo( 'body').addClass('tour-icon flyable').css({'position':'fixed','z-index':3001,'top':$('.map-tour-selector #'+target+' .icon').offset().top,'left':$('.map-tour-selector #'+target+' .icon').offset().left});
            }
            else {
                $('.map-tour-selector #'+target+' .icon').clone().appendTo( 'body').addClass('tour-icon flyable').css({'position':'fixed','z-index':3001,'top':$(this).offset().top,'left':$(this).offset().left});
            }
            $('.map-tour-selector #'+target+' .icon').not('.flyable').remove();
            $('body .icon.flyable').animate({
                opacity: 1,
                left: ($(window).width()>768 ? parseInt($('.menu-container .logo').offset().left+134) : parseInt($(window).width()/2)+60),
                top: ($(window).width()>768 ? 20 : 25)
                }, 1000, function() {
                // Animation complete.
                location.href=href
            });
            //setTimeout(function(){location.href=href},1000);
        });

        $('.header-image .search .search-open').on('click',function(){
            $('.header-image .search .search-input,.mobile-search-input').addClass('active');
        });

        $('.header-image .search .search-close,.mobile-search-input .search-close').on('click',function(){
            $('.header-image .search .search-input,.mobile-search-input').removeClass('active');
            location.href=$(this).closest('.search-input,.mobile-search-input').find('input').data('url');
        });
        
         $('.header-image .search .search-input input,.mobile-search-input input').on('keypress',function(e){
             if (window.event) keycode = window.event.keyCode;
            else if (e) keycode = e.which;
            //else return true;
            if (keycode == 13)
            {
                e.preventDefault();
                location.href=$(this).data('url')+'?search='+$(this).val();
            }
         });
         
