        $('#kektura-form').on('submit', function(e){
            e.preventDefault();
            var errors = [];
            var $form = $(this);
            if ($(this).find('input[name="DataStore[name_given]"]').val().length<1) errors.push('Add meg a neved!');
            if ($(this).find('input[name="DataStore[email]"]').val().length<4) errors.push('Add meg e-mail címedet!');
            if (!$(this).find('input[name="acceptPrivacy"]').prop('checked')) errors.push('Feliratkozáshoz el kell fogadnod az adatvédelmi irányelveinket!');
            if (errors.length==0) {
                $form.find('.message').html('');
                $.ajax({
                    url: '/action/do/205',
                    data: $form.serialize(),
                    type: 'post',
                    dataType: 'json',
                    beforeSend: function(){
                        $form.find('button').prop('disabled', true);
                    },
                    complete: function(){
                        $form.find('button').prop('disabled', false);
                    },
                    success: function(e) {
                        if (e.status == 'ok') {
                            $form.find('.message').html(e.message);
                        } else {
                            $form.find('.message').html(e.message);
                        }
                    },
                    error: function() {
                        window.location.reload();
                    }
                });
            } else {
                $form.find('.message').html('<div class="alert alert-danger">'+errors.join('<br>')+'</div>');
            }

        });


        $('.next-step').on('click', function(e){
            e.preventDefault();
            $('#ticket-form').submit();
        });
        
        function initModalSettings(modalid) {
            $('body > .page-content,body > .page-block, .modalcnt,.newsletter-block').hide();
                $('.navbar-main').removeClass('in');
                $('#mainMenu').removeClass('opened');
                $('#'+modalid+' .container').addClass('white');
                if($('.header-image.subpage').length>0) {
                    $('#'+modalid+' .modal-title').hide();
                    $('.header-image h1').html($('#'+modalid+' .modal-title').html());
                }
        }

        $(document).on('click', '.mobile-menu', function(e){
            e.preventDefault();
            if ($('body').hasClass('menu-opened')) {
                $('body').removeClass('menu-opened');
            }
            else {
                $('body').addClass('menu-opened');
            }
            
        });

        $(document).on('click', '.modalcnt-close, .modalcnt-close-link', function(e){
            e.preventDefault();
            if ($(this).closest('.modalcnt').hasClass('reload')) window.location.reload(true);
            else $(this).closest('.modalcnt').fadeOut(100);
            if ($(this).closest('.modalcnt').attr('id') == 'modal-login') {
                $('#modal-login').find('.lost-password').hide();
                $('#modal-login').find('.login').show();
            }
        });
        $(document).ajaxError(function(event, request, settings) {
            if (request.statusText == "abort") return;
            var inurl = settings.url.indexOf(window.location.host);
            if (((settings.url[0]=='/') && (settings.url[1]!='/')) || ((inurl>-1) && (inurl<10))) {
                if (request.status == 412) {
                    console.debug(event, request, settings);
                    //window.location.href='https://www.gerecse50.hu/bejelentkezes-regisztracio?message=K%C3%A9rlek+jelentkezz+be%21&returnUrl='+encodeURI(window.location.href);
                }
            }
        });

        

        $('#newsletter-form a.btn').on('click',function(e){e.preventDefault();$('#newsletter-form').submit();});

        $('#newsletter-form').on('submit', function(e){
            e.preventDefault();
            var errors = [];
            var $form = $(this);
            var data = $form.serialize();
            //if ($(this).find('input[name="DataStore[name_given]"]').val().length<1) errors.push('Add meg a neved!');
            if ($(this).find('input[name="DataStore[email]"]').val().length<4) errors.push('Add meg e-mail címedet!');
            if ($(this).find('input[name="DataStore[name_given]"]').val().length==0) errors.push('Add meg a nevedet!');
            if (!$(this).find('input[name="acceptPrivacy"]').prop('checked')) errors.push('Feliratkozáshoz el kell fogadnod az adatvédelmi irányelveinket!');
            if (errors.length==0) {
                $form.find('.message').html('');
                $.ajax({
                    url: '/action/do/493',
                    data: data,
                    type: 'get',
                    dataType: 'json',
                    beforeSend: function(){
                        $form.find('.button').prop('disabled', true);
                    },
                    complete: function(){
                        $form.find('.button').prop('disabled', false);
                    },
                    success: function(e) {
                        if (e.status == 'ok') {
                            $form.find('.message').html(e.message);
                            $form.find('input[name="DataStore[email]"]').val('');
                            $form.find('input[name="DataStore[name_family]"]').val('');
                            $form.find('input[name="DataStore[name_given]"]').val('');
                            $form.find('input[name="acceptPrivacy"]').prop('checked', false);
                        } else {
                            $form.find('.message').html(e.message);
                        }
                    },
                    error: function(e) {
                        $form.find('.message').html(e.message);
                        //window.location.reload();
                    }
                });
            } else {
                $form.find('.message').html('<div class="alert alert-danger">'+errors.join('<br>')+'</div>');
            }

        });

        $('.contact-form form').on('submit', function(e){
            e.preventDefault();
            var errors = [];
            var $form = $(this);
            var error = false;

            if ($(this).find('input[name="name"]').val()=='') {
                error = true;
                errors.push('Név kitöltése kötelező!');
            }
            if ($(this).find('input[name="email"]').val()=='' && $(this).find('input[name="tel"]').val()=='') {
                error = true;
                errors.push('E-mail vagy telefonszám kitöltése kötelező!');
            }
            if ($(this).find('textarea').val()=='') {
                error = true;
                errors.push('Üzenet kitöltése kötelező!');
            }

            if (errors.length==0) {
                $form.find('.message').html('');
                $.ajax({
                    url: '/action/do/572',
                    data: $form.serialize(),
                    type: 'post',
                    dataType: 'json',
                    beforeSend: function(){
                        $form.find('.btn').prop('disabled', true);
                    },
                    complete: function(){
                        $form.find('.btn').prop('disabled', false);
                    },
                    success: function(e) {
                        if (e.status == 'ok') {
                            $form.find('.message').html('');
                            //showNotify('', 'success',e.message);
                            $form.find('.message').html('<div class="alert alert-success">'+e.message+'</div>');
                        } else {
                            $form.find('.message').html('<div class="alert alert-warning">'+e.message+'</div>');
                        }
                    },
                    error: function(e) {
                        $form.find('.message').html('<div class="alert-warning">'+e.message+'</div>');
                        //window.location.reload();
                    }
                });
            } else {
                $form.find('.message').html('<div class="alert alert-danger">'+errors.join('<br>')+'</div>');
            }

        });
        
        $('ul.dropdown-menu [data-toggle=dropdown]').on('click', function(event) {
            event.preventDefault();
            event.stopPropagation();
            $(this).parent().siblings().removeClass('open');
            $(this).parent().toggleClass('open');
        });


