           window.addEventListener('popstate', function(e) {
              var state = e.state;
              if (state == null || state === null) {
                  $('body').removeClass('noscroll');
                  if($('#modaltransparent').length) {
                      $('#modaltransparent').remove();
                  }
                  else {
                     location.reload();
                  }
                  
              } else {
                  arr = state.split('|');
                  $('body').removeClass('noscroll');
                  if(arr[0]=='gallery'){
                    redirecturl = location.href;
                    if(!$('#gallery-'+arr[1]).length) openGalleryLayer(arr[1],arr[2],redirecturl);
                    updateSliderIndex(arr[2]);
                  }
                  else if(arr[0]=='fpall'){
                    redirecturl = location.href;
                    if(!$('#gallery-'+arr[1]).length) openFotoPalyazat(arr[1],arr[2],redirecturl);
                    updateSliderIndex(arr[2]);
                  }
                  else if(arr[0]=='video'){
                    redirecturl = location.href;
                    if(!$('#gallery-'+arr[1]).length) openVideoGallery(arr[1],arr[2],redirecturl,'video');
                    updateSliderIndex(arr[2]);
                  }
                  else if(arr[0]=='videogallery'){
                    redirecturl = location.href;
                    if(!$('#gallery-'+arr[1]).length) openVideoGallery(arr[1],arr[2],redirecturl,'videogallery');
                    updateSliderIndex(arr[2]);
                  }
                  else if(arr[0]=='modal' && arr[1]){
                      $('body').removeClass('noscroll');
                      //console.log('modal'+arr[1]);
                      if(arr[1]=='belyegzohelyszakaszok&group=belyegzohelyszakaszok'){
                          $btn = $('.szakasz-modal img');
                          tableModal($btn);
                      } 
                      else if(arr[1]=='close') {
                          $modal = $('#'+arr[2]);
                          $modal.modal('hide');
                          //var $this = $(this);
                          //$this.closest('.modal').modal('hide');
                      }
                      else {
                        var $btn = $('.btn-show-modal[data-btnid="'+arr[1]+'"]');
                        if($btn) modalInit($btn,1);
                      }
                  }
                  
              }
             
            }); 
     });
</script>
